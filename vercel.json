{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "avatar/index.html", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/dashboard/index.html" },
    { "src": "/avatar", "dest": "/avatar/index.html" },
    { "src": "/dashboard", "dest": "/dashboard/index.html" }
  ]
}
